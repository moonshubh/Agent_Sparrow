  "authenticationErrors": {
    "Unknown": {
      "description": "Unknown authentication error",
      "userAction": "Check credentials and try again. Contact support if issue persists."
    },
    "NoAuthenticationMethods": {
      "description": "No supported authentication methods found",
      "userAction": "Server may not support required auth. Update Mailbird or contact support."
    },
    "BadCredentials": {
      "description": "Invalid username or password",
      "userAction": "Verify credentials. Use app password if 2FA enabled. Check for typos."
    },
    "GmailRequires2StepAuthentication": {
      "description": "Gmail requires 2-step verification setup",
      "userAction": "Enable 2FA on Google account and generate app-specific password, or use OAuth2."
    },
    "GmailLikelyLessSecureAppsIssue": {
      "description": "Google's Less Secure Apps access issue",
      "userAction": "Use OAuth2 authentication or generate app-specific password."
    },
    "GmailWebLoginRequired": {
      "description": "Google requires web browser verification",
      "userAction": "Sign in via web browser first, then try Mailbird again."
    },
    "Pop3MailboxInUse": {
      "description": "POP3 mailbox already accessed by another client",
      "userAction": "Close other email clients accessing this account and wait 10-15 minutes."
    },
    "Pop3ExceededLoginLimit": {
      "description": "Login limit exceeded",
      "userAction": "Wait before trying again. POP3 has login frequency limits."
    },
    "BandwidthLimitExceeded": {
      "description": "Provider bandwidth limit reached",
      "userAction": "Wait and try again later. Reduce sync frequency."
    },
    "GoogleBandwidthLimitExceeded": {
      "description": "Google bandwidth limit exceeded",
      "userAction": "Google limits downloads. Wait 24 hours or reduce sync frequency."
    },
    "PossibleAntivirusInterference": {
      "description": "Antivirus may be interfering with connection",
      "userAction": "Temporarily disable antivirus/firewall and test. Add Mailbird exception."
    },
    "ConnectionTimeout": {
      "description": "Connection timed out",
      "userAction": "Check internet. Verify server settings. Check firewall."
    },
    "CertificateValidationFailed": {
      "description": "SSL certificate validation failed",
      "userAction": "Check system date/time. Update certificates. Try different security protocol."
    },
    "RateLimitExceeded": {
      "description": "Too many requests to server",
      "userAction": "Wait before retrying. Reduce sync frequency."
    },
    "AccountTemporarilyLocked": {
      "description": "Account temporarily locked by provider",
      "userAction": "Follow provider's account recovery/unlock procedure."
    },
    "ServerCapacityIssue": {
      "description": "Server capacity or resource issue",
      "userAction": "Server is overloaded. Try again later."
    },
    "YahooAndCoLogoutException": {
      "description": "Yahoo session unexpectedly logged out",
      "userAction": "Re-authenticate in Yahoo/OAuth, or generate a new app password; if it recurs, reauthorize the account in Mailbird."
    }
  },
  "oAuth2Errors": {
    "Unknown": {
      "description": "Unknown OAuth2 error",
      "userAction": "Try authorization again. Clear browser cache if issues persist."
    },
    "UserCancelledOrTimedOut": {
      "description": "Authorization cancelled or timed out",
      "userAction": "Complete authorization within time limit. Don't close browser during auth."
    },
    "AccountChanged": {
      "description": "Different account authorized than expected",
      "userAction": "Sign out of other accounts in browser, then authorize with correct account."
    },
    "TokenRetrieval": {
      "description": "Failed to retrieve authorization tokens",
      "userAction": "Try again. Check internet connection. Clear browser cache."
    },
    "Connection": {
      "description": "Connection error during authorization",
      "userAction": "Check internet connection and try again."
    },
    "HttpsRedirect": {
      "description": "HTTPS redirect issue",
      "userAction": "Try with different browser. Clear browser cache and cookies."
    }
